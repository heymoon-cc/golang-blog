{{define "title"}}{{ .Tag }}{{end}}

{{define "main"}}
  {{ if .Articles }}
      {{ range .Articles }}
      <div class="row g-5">
        <div class="col-md-8">
          <article class="blog-post">
            <h2 class="blog-post-title">{{ .Title }}</h2>
              {{ .Content }}
          </article>
        </div>
      </div>
    {{ end }}
  {{ end }}
{{end}}
{{template "base" .}}
